{
  "name": "rudder-snowplow-plugin",
  "version": "1.0.0",
  "description": "A plugin to help snowplow migration",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildDevAdapter": "NODE_ENV=false uglify=false rollup -c",
    "buildProdAdapter": "NODE_ENV=false PROD_DEBUG=true STAGING=$npm_config_staging ENV=prod uglify=true rollup -c",
    "buildProdAdapterDebug": "NODE_ENV=false ENV=prod PROD_DEBUG_INLINE=true uglify=true rollup -c",
    "buildProdAdapterGzip": "NODE_ENV=false  ENV=prod ENC=gzip uglify=true rollup -c",
    "buildProdAdapterBrotli": "NODE_ENV=false  ENV=prod ENC=br uglify=true rollup -c",
    "buildForNpm": "NPM=true rollup -c",
    "bundle-size-visual": "ENV=prod PROD_DEBUG=true uglify=true visualizer=true rollup -c",
    "size": "npm run clean && npm run buildProdAdapter && npm run buildAllIntegrations && npm run buildForNpm && size-limit",
    "changelog": "auto-changelog -p -t keepachangelog -u true -l false --sort-commits date-desc ",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint . --fix",
    "lint:check": "eslint . || exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-snowplow-plugin.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-snowplow-plugin/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-snowplow-plugin#readme",
  "dependencies": {
    "rudder-sdk-js": "^2.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-arrow-functions": "^7.17.12",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@size-limit/preset-big-lib": "^7.0.8",
    "auto-changelog": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "rollup": "^2.75.3",
    "rollup-plugin-brotli": "^3.1.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-gzip": "^3.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "size-limit": "^7.0.8"
  }
}
